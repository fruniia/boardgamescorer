<table class="table">
	<thead>
		<tr>
			<th scope="col"></th>
			<th scope="col">Terrain</th>
			<th scope="col">No of tiles</th>
			<th scope="col">No of crowns</th>
		</tr>
	</thead>
	<tbody>

		@foreach (var game in Registration)
		{
			<tr>
				<th scope="row"></th>
				<td>@game.Terrain</td>
				<td>@game.Tiles</td>
				<td>@game.Crowns</td>
			</tr>
		}
	</tbody>
	</table>
@code {
	[Parameter]
	public List<KingDominoGame>? Registration { get; set; }
}
